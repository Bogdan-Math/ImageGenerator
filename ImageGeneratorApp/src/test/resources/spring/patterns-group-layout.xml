<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="imagesLayout" class="layers.web.vaadin.layout.patterns.PatternsGroupLayout"/>
    <bean id="patternsGroup" class="layers.web.vaadin.layout.patterns.group.PatternsGroup"/>
    <bean id="basicSettings" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="domain.BasicSettings"/>
    </bean>
    <bean id="cachedPatternsService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="layers.service.CachedPatternsService"/>
    </bean>

</beans>